extends layout 

block content 
    header.w3-container.w3-teal
        h1 Compositor #{compositor.id}
    .w3-container 
        ul.w3-ul.w3-card-4(style="width:50%")
            li 
                b Nome: #{compositor.nome}
            li 
                b Id: #{compositor.id}
            li 
                b Data Nascimento: #{compositor.dataNasc}
            li 
                b Data Obito: #{compositor.dataObito}
            li 
                b Periodo: #{compositor.periodo}
            li 
                b Biografia: #{compositor.bio}
    footer.w3-container.w3-teal 
        address 
            span Generated by Andre Filipe in #{date} - [
            a(href="/compositores") Voltar 
            span ]
